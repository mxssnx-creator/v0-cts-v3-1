{
  "name": "cts",
  "version": "3.1.0",
  "private": true,
  "description": "Comprehensive Cryptocurrency Trading System - Multi-exchange automated trading platform",
  "keywords": [
    "cryptocurrency",
    "trading",
    "automated-trading",
    "bybit",
    "bingx",
    "pionex",
    "orangex",
    "trading-bot",
    "crypto-bot",
    "algorithmic-trading",
    "multi-exchange",
    "npx",
    "cli"
  ],
  "homepage": "https://github.com/your-username/cts-v3.1#readme",
  "bugs": {
    "url": "https://github.com/your-username/cts-v3.1/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/cts-v3.1.git"
  },
  "license": "UNLICENSED",
  "author": "CTS Development Team",
  "bin": {
    "cts-download": "./scripts/quick-install.sh",
    "cts-install": "./scripts/install.sh",
    "cts-npx-install": "./scripts/npx-install.sh",
    "cts-setup": "./scripts/setup.js"
  },
  "scripts": {
    "prebuild": "node scripts/prebuild.js",
    "build": "next build",
    "certbot:install": "sudo bash scripts/install-certbot.sh",
    "clean": "bash scripts/clear-cache.sh",
    "clean:aggressive": "node scripts/prebuild.js",
    "clean:all": "bash scripts/clear-cache.sh && rm -rf node_modules && npm install",
    "clean:local": "bash scripts/clear-local-cache.sh",
    "clean:vercel": "rm -rf .next .turbo node_modules/.cache tsconfig.tsbuildinfo",
    "db:backup": "node scripts/backup-database.js",
    "db:migrate": "node scripts/run-migrations.js",
    "db:reset": "node scripts/reset-database.js",
    "db:status": "node scripts/check-db-status.js",
    "dev": "bash scripts/clear-local-cache.sh && next dev -p ${PORT:-3000}",
    "dev:nocache": "next dev -p ${PORT:-3000}",
    "preinstall": "rm -rf .next .turbo tsconfig.tsbuildinfo node_modules/.cache 2>/dev/null || true",
    "postinstall": "npm rebuild better-sqlite3 sharp 2>/dev/null || true",
    "install:download": "bash scripts/quick-install.sh",
    "install:full": "bash scripts/install.sh",
    "install:quick": "bash scripts/install-continuous.sh",
    "lint": "next lint",
    "nginx:logs": "sudo tail -f /var/log/nginx/error.log",
    "nginx:restart": "sudo systemctl restart nginx",
    "nginx:setup": "sudo bash scripts/setup-nginx.sh",
    "nginx:status": "sudo systemctl status nginx",
    "predeploy": "npm run verify",
    "rebuild": "npm run clean && npm run build",
    "setup": "node scripts/setup.js",
    "start": "next start -p ${PORT:-3000}",
    "system:check": "node scripts/system-check.js",
    "system:health": "node scripts/health-check.js",
    "trade-engine": "node services/trade-engine.js",
    "type-check": "tsc --noEmit",
    "vercel-build": "npm run clean:aggressive && npm run type-check && npm run build",
    "verify": "node scripts/verify-build.js",
    "web-engine": "node services/web-engine.js"
  },
