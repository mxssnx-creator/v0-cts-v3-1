{
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "env": {
    "NODE_ENV": "development",
    "NODE_PG_FORCE_NATIVE": "false",
    "npm_config_build_from_source": "false",
    "SKIP_OPTIONAL_DEPS": "true",
    "NEXT_PUBLIC_APP_URL": "https://v0-cts-v3-1.vercel.app"
  },
  "envFrom": [
    "REMOTE_POSTGRES_URL",
    "DATABASE_URL",
    "SESSION_SECRET",
    "JWT_SECRET",
    "ENCRYPTION_KEY",
    "API_SIGNING_SECRET",
    "NODE_ENV",
    "DATABASE_TYPE"
  ],
  "framework": "nextjs",
  "regions": ["iad1"]
}
