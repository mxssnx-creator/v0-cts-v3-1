{
  "buildCommand": "npm run vercel-build",
  "installCommand": "npm install",
  "env": {
    "NODE_ENV": "production",
    "NODE_PG_FORCE_NATIVE": "false",
    "npm_config_build_from_source": "false",
    "SKIP_OPTIONAL_DEPS": "true"
  },
  "framework": "nextjs",
  "regions": ["iad1"],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ]
}
